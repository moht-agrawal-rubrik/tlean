
# GitHub Configuration
# Required: GitHub personal access token for API access
GITHUB_TOKEN=your_github_token_here

# Required: GitHub username to fetch PRs for
GITHUB_USERNAME=your_github_username

# Optional: Default limit for number of PRs to fetch (default: 10)
GITHUB_LIMIT=10

# Optional: OpenAI API configuration for LLM-based analysis
OPENAI_API_KEY=your_openai_api_key_here
# Slack OAuth Configuration
# Get your OAuth token from https://api.slack.com/apps
SLACK_OAUTH_TOKEN=xoxp-***

# Optional: Slack App Configuration
SLACK_CLIENT_ID=your-client-id
SLACK_CLIENT_SECRET=your-client-secret
SLACK_SIGNING_SECRET=your-signing-secret

# FastAPI Configuration
DEBUG=true
HOST=0.0.0.0
PORT=8000

# Logging Configuration
LOG_LEVEL=INFO
